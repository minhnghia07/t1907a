create table product(
     id int primary key auto_increment,
     nameproduct varchar (100),
     link varchar(500),
     price float,
     installment float,
     title varchar(100)
)
create table contact(
    id int primary key auto_increment,
    nameproduct varchar(50),
    contact varchar (2000)
)
create table userr(
    id int primary key auto_increment,
    username varchar(100),
    email varchar(100),
    fullname varchar (100),
    phone int (10),
    password varchar(50)
)